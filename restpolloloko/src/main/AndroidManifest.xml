<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sxtsoft.restpolloloko">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DestPedidos"
            android:label="@string/title_activity_dest_pedidos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".CrearProducto"
            android:label="@string/title_activity_crear_producto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CrearCamarero"
            android:label="@string/title_activity_crear_camarero"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DestProductos"
            android:label="@string/title_activity_dest_productos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DestCamareros" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>